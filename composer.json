        },
        {
            "type": "git",
            "url": "git@github.com:villahide/wp-plugin-bestel-core.git"
        },
        {
            "type": "git",
            "url": "git@github.com:villahide/wp-plugin-bastel-review.git"
        },
        {
            "type": "git",
            "url": "git@github.com:villahide/wp-plugin-bestel-demo-installer.git"
        },
        {
            "type": "git",
            "url": "git@github.com:villahide/wp-plugin-hotel-booking.git"
